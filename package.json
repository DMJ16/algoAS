{
  "name": "algoAS",
  "version": "0.0.1",
  "description": "Practice algorithms and data structures in AssemblyScript.",
  "main": "index.ts",
  "scripts": {
    "test": "asp --verbose",
    "asbuild:untouched": "asc assembly/index.as.ts --target debug",
    "asbuild:optimized": "asc assembly/index.as.ts --target release",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "asbuild:wasi": "asc assembly/index.as.ts -b build/algoAS.wasm -t build/algoAS.wat --runtime half",
    "wasmtime": "wasmtime --dir . build/algoAS.wasm",
    "start": "npm run asbuild:wasi && npm run wasmtime",
    "asbuild:watch": "onchange -i 'assembly/**/*' -- npm run start"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/dmj16/algoAS"
  },
  "keywords": [
    "assemblyscript",
    "webassembly",
    "WASI",
    "algorithms",
    "data structures"
  ],
  "author": "david josephs",
  "license": "ISC",
  "dependencies": {
    "@assemblyscript/loader": "^0.14.8",
    "@guildofweavers/merkle": "^0.3.12",
    "as-bignum": "^0.2.3",
    "as-bind": "^0.4.0",
    "as-wasi": "^0.2.0",
    "assemblyscript": "^0.14.8"
  },
  "devDependencies": {
    "@as-pect/cli": "^4.0.0",
    "@assemblyscript/node": "github:AssemblyScript/node",
    "@types/node": "^14.0.27",
    "onchange": "^7.0.2"
  }
}
